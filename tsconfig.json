{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "isolatedModules": true,
    "lib": [
      "dom",
      "esnext"
    ],
    "allowJs": false,
    "noImplicitAny": true,
    "noEmit": true
  },
  "include": [
    "solutions",
    "scripts"
  ],
  "exclude": [
    "node_modules"
  ]
}
